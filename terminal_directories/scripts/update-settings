#!/bin/bash

set -e

white='\e[0;37m'
yellow='\e[0;33m'

echo -e "${yellow} cd $HOME/myubuntu${white}"
cd $HOME/myubuntu
echo -e "${yellow} git fetch${white}"
git fetch
echo -e "${yellow} git merge origin/master${white}"
git merge origin/master
echo -e "${yellow} apply-terminal${white}"
apply-terminal
